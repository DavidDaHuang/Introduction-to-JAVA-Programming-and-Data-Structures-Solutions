public class Exercise01_07 {
  public static void main(String args[]) {
    System.out.println(4 * (1.0 - (double)1/3 + (double)1/5 - (double)1/7 + (double)1/9 - (double)1/11));
    System.out.println(4 * (1.0 - (double)1/3 + (double)1/5 - (double)1/7 + (double)1/9 - (double)1/11 + (double)1/13));
  }
}